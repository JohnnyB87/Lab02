<?import javafx.scene.layout.*?>
<?import controllers.ListOfWinnersTab?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.Button?>
<ListOfWinnersTab xmlns="http://javafx.com/javafx"
                  xmlns:fx="http://javafx.com/fxml"
                  fx:controller="controllers.ListOfWinnersTab">
    <Pane fx:id="WinnersPane" style="-fx-background-color: #bcbcbc;" prefWidth="410" prefHeight="350">
        <TableView fx:id="WinnersTableView" layoutX="50" layoutY="30" maxHeight="200" maxWidth="300">
            <columns>
                <TableColumn prefWidth="100.0" text="First Name" fx:id="FirstName" />
                <TableColumn prefWidth="100.0" text="Last Name" fx:id="LastName" />
                <TableColumn prefWidth="100.0" text="Star Prize" fx:id="StarPrize" />
            </columns>
        </TableView>
        <Button fx:id="RemoveButton" text="Remove" layoutX="75" layoutY="250" onAction="#removeButtonPressed"/>
    </Pane>
</ListOfWinnersTab>
